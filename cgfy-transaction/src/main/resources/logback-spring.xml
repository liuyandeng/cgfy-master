<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <withJansi>true</withJansi>
        <encoder>
            <pattern>Data:%d{yyyy-MM-dd HH:mm:ss} Level:%-5level File:[%F] -- Method:%M:%L -- %msg%n</pattern>
<!--            <pattern>%M:%L -&#45;&#45; %msg%n</pattern>-->
            <charset>UTF-8</charset>
        </encoder>
    </appender>
    <root level="DEBUG">
        <appender-ref ref="STDOUT"/>
    </root>

</configuration>
